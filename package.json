{
  "name": "test-paper-editor",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Uni-app based test paper editor with flow engine and routing center.",
  "scripts": {
    "dev": "npm run dev:h5",
    "build": "npm run build:h5",
    "preview": "npm run preview:h5",
    "dev:h5": "vite",
    "build:h5": "vite build",
    "preview:h5": "vite preview",
    "test": "node --test",
    "test:flow": "node --test tests/flow-profile-routing.test.mjs",
    "test:runtime": "node --test tests/runtime-trace.test.mjs",
    "test:guard": "node --test tests/store-guardrails.test.mjs",
    "test:visual-compiler": "node --test tests/flow-visual-compiler.test.mjs",
    "test:visual-module": "node --test tests/flow-visual-module-mapper.test.mjs",
    "test:visual-history": "node --test tests/flow-visual-history.test.mjs"
  },
  "dependencies": {
    "@vue-flow/background": "^1.3.2",
    "@vue-flow/controls": "^1.1.2",
    "@vue-flow/core": "^1.41.5",
    "dagre": "^0.8.5",
    "vue": "^3.5.13",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "vite": "^5.4.10"
  }
}
